  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && gatsby build",
    "develop": "gatsby develop",
    "dev": "export SET NODE_OPTIONS=--openssl-legacy-provider && npm run develop",
    "start": "export SET NODE_OPTIONS=--openssl-legacy-provider && npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.8.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-google-analytics": "^5.10.0",
    "prettier": "2.1.1",
    "tailwindcss": "^2.1.1"
  }
